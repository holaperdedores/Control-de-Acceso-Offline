        app:layout_constraintTop_toBottomOf="@+id/lblCargo"
        android:onClick="onclickTxtEscanear">
</EditText>
    <!--Mensaje-->
    <TextView
        android:id="@+id/textView4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:gravity="center"
        android:text="En caso de problema digite el rut"
        android:textColor="@color/negroTitulo"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtEscanear" />
    <!--Camara-->
    <Button
        android:id="@+id/btnCamara"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:background="@drawable/ic_baseline_camera_alt_24"
        android:onClick="onclickCamara"
        app:backgroundTint="@color/azulBotonIngresos"
        app:layout_constraintEnd_toStartOf="@+id/btnSincronizar"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />
    <!--Teclado-->
    <!--Volver-->
    <Button
        android:id="@+id/btnTeclado"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/ic_baseline_keyboard_24"
        android:onClick="onclickTeclado"
        app:backgroundTint="@color/azulBotonIngresos"
        app:layout_constraintStart_toEndOf="@+id/btnSincronizar"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <Button
        android:id="@+id/btnPanelCerrarGrupo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="24sp"
        android:layout_marginEnd="24sp"
        android:layout_marginBottom="10sp"
        android:onClick="onclickPanelCerrar"
        android:text="cerrar vehiculo"
        android:textSize="14sp"
        android:background="@drawable/nalfastrap_button_radius_border"
        app:backgroundTint="@color/verdeBotonVisitas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/btnPanelCentral"
        />
    <Button
        android:id="@+id/btnPanelCentral"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="24sp"
        android:layout_marginEnd="24sp"
        android:layout_marginBottom="10sp"
        android:onClick="onclickPanelCentral"
        android:text="PANEL CENTRAL"
        android:textSize="14sp"
        android:background="@drawable/nalfastrap_button_radius_border"
        app:backgroundTint="@color/rojoPanelCentral"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        />
    <!--TecladoVirtual--><TableLayout
        android:id="@+id/tablaTeclado"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:visibility="gone">
        <TableRow
        android:layout_width="match_parent"
